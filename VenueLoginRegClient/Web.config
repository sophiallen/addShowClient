<configuration>

    <system.web>
      <compilation debug="true" targetFramework="4.5.2" />
      <httpRuntime targetFramework="4.5.2" />
    </system.web>

    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IVenueLoginRegService" />
                <binding name="BasicHttpBinding_IShowTrackerService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:5698/VenueLoginRegService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IVenueLoginRegService"
                contract="LoginRegSvcRef.IVenueLoginRegService" name="BasicHttpBinding_IVenueLoginRegService" />
            <endpoint address="http://localhost:6060/ShowTrackerService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IShowTrackerService"
                contract="SelectServiceRef.IShowTrackerService" name="BasicHttpBinding_IShowTrackerService" />
        </client>
    </system.serviceModel>
</configuration>
